{
    "name": "youloop",
    "version": "1.0.0",
    "description": "Loop sections of youtube videos to practice music",
    "main": "index.js",
    "scripts": {
        "browserify": "./scripts/browserify.sh",
        "dev": "npm run browserify && web-ext run",
        "watch": "npm run browserify && chokidar './src/**/*.js' -i './src/**/bundle.js' -c 'npm run browserify'",
        "format_check": "prettier --check ./src/",
        "format": "prettier --write ./src/",
        "lint": "eslint_d ./src/"
    },
    "keywords": [],
    "author": "Lyonel Zamora",
    "license": "ISC",
    "devDependencies": {
        "@fsouza/prettierd": "^0.22.2",
        "browserify": "^17.0.0",
        "chokidar-cli": "^3.0.0",
        "eslint_d": "^12.2.1",
        "esmify": "^2.1.1",
        "prettier": "^2.7.1",
        "web-ext": "^7.2.0"
    },
    "dependencies": {
        "hh-mm-ss": "^1.2.0",
        "tone": "^14.7.77"
    }
}
